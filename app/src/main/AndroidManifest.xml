<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.a11630"
    android:versionCode="2"
    android:versionName="2.0"
    tools:ignore="GradleOverrides">

    <uses-feature android:name="android.hardware.camera" />
    <!--相机权限-->
    <uses-permission android:name="android.permission.CAMERA" />
    <!--写入SD卡的权限：如果你希望保存相机拍照后的照片-->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!--读取SD卡的权限:打开相册选取图片所必须的权限-->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.INTERNET"/>
    <!--网络-->

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".face_new.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".face_new.ConnActivity_teacher">
        </activity>

        <activity android:name=".face_new.ConnActivity_student">
        </activity>


        <activity
            android:name=".face_new.in"
            android:label="录入">
        </activity>

        <activity
            android:name=".face_new.opt"
            android:label="打卡">
        </activity>

        <activity
            android:name=".face_new.searcher"
            android:label="查询">
        </activity>

        <activity
            android:name=".face_new.delete"
            android:label="删除">
        </activity>

    </application>

</manifest>